{
  "name": "angular",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' '__tests__/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "jest": "jest --watch --notify",
    "tsc": "tsc",
    "build": "webpack -p --progress --profile"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "verbose": true
  },
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/es6-promise": "0.0.33",
    "@types/jest": "20.0.3",
    "chai": "4.1.2",
    "jest": "21.0.1",
    "jquery.cookie": "1.4.1",
    "npm": "5.4.1",
    "rxjs": "5.4.3",
    "systemjs": "0.20.18",
    "systemjs-builder": "0.16.10",
    "ts-jest": "21.0.0",
    "ts-loader": "2.3.7",
    "tslint": "5.7.0",
    "typescript": "2.5.2",
    "webpack": "3.5.6"
  }
}
