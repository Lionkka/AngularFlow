{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": [
            true,
            "check-format",
            "allow-leading-underscore"
        ],
        "interface-name": [true, "never-prefix"],
        "no-namespace": [true, "allow-declarations"]
    },
    "rulesDirectory": []
}